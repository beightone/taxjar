{
  "stack": "dotnet",
  "memory": 256,
  "runtimeArgs": [
  ],
  "routes": {
    "ratesForLocation": {
      "path": "/taxjar/rates/:zip",
      "public": true
    },
    "categories": {
      "path": "/taxjar/categories",
      "public": true
    },
    "taxForOrder": {
      "path": "/taxjar/tax-for-order",
      "public": true
    },
    "taxjarOrderTaxHandler": {
      "path": "/taxjar/checkout/order-tax",
      "public": true
    },
    "processInvoiceHook": {
      "path": "/taxjar/oms/invoice",
      "public": true
    },
    "initConfig": {
      "path": "/taxjar/init-config",
      "public": true
    },
    "summaryRates": {
      "path": "/taxjar/summary-rates",
      "public": true
    }
  },
  "events": {
    "onAppsLinked": {
      "sender": "apps",
      "keys": [ "linked" ]
    },
    "allStates": {
      "sender": "vtex.orders-broadcast",
      "topics": [ "order-status-updated" ]
    }
  }
}
