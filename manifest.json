{
  "name": "taxjar",
  "vendor": "vtex",
  "version": "0.0.1",
  "title": "",
  "description": "",
  "categories": [],
  "settingsSchema": {
    "title": "Taxjar",
    "type": "object",
    "properties": {
      "apiToken": {
        "title": "Api Token",
        "description": "Api Token",
        "type": "string"
      },
      "isLive": {
        "title": "Production",
        "description": "Production",
        "type": "boolean"
      },
      "enableTransactionPosting": {
        "title": "Enable Transaction Posting",
        "description": "Record invoiced orders in Taxjar",
        "type": "boolean"
      }
    }
  },
  "registries": [
    "smartcheckout"
  ],
  "builders": {
    "dotnet": "2.x",
    "graphql": "1.x"
  },
  "mustUpdateAt": "2018-09-05",
  "scripts": {
    "postreleasy": "vtex publish --verbose"
  },
  "policies": [
    {
      "name": "outbound-access",
      "attrs": {
        "host": "bnb.data.bl.uk"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "nuget.org"
      }
    },
    {
      "name": "ADMIN_DS"
    },
    {
      "name": "LogisticsViewer"
    },
    {
      "name": "OMSViewer"
