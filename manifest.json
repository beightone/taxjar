{
  "name": "taxjar",
  "vendor": "vtex",
  "version": "0.0.1",
  "title": "",
  "description": "",
  "categories": [],
  "settingsSchema": {
    "title": "TaxJar",
    "type": "object",
    "properties": {
      "apiToken": {
        "title": "Api Token",
        "description": "Api Token - https://app.taxjar.com/api_sign_up",
        "type": "string"
      },
      "isLive": {
        "title": "Production",
        "description": "Production",
        "type": "boolean"
      },
      "enableTaxCalculations": {
        "title": "Enable Tax Calculations",
        "description": "Enable live tax calculations",
        "type": "boolean",
        "default": true
      },
      "enableTransactionPosting": {
        "title": "Enable Transaction Posting",
        "description": "Record invoiced orders in TaxJar",
        "type": "boolean",
        "default": true
      }
    }
  },
  "billingOptions": {
    "termsURL": "https://compliance.vtex.com/gdpr/policies/vtex-privacy-policy",
    "support": {
      "url": "https://help-tickets.vtex.com/en/support?app=vtex.taxjar"
    },
    "free": true,
    "type": "free",
    "availableCountries": [
      "*"
    ]
  },
  "registries": [
    "smartcheckout"
  ],
  "builders": {
    "dotnet": "2.x",
    "graphql": "1.x",
    "docs": "0.x"
  },
  "mustUpdateAt": "2018-09-05",
  "scripts": {
    "postreleasy": "vtex publish --verbose"
  },
  "policies": [
    {
      "name": "outbound-access",
      "attrs": {
        "host": "bnb.data.bl.uk"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "nuget.org"
      }
